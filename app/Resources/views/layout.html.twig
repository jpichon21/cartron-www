    <script>
      // Script to manage cookies
      tarteaucitron.init({
        "privacyUrl": "{{ path('privacy_policy') }}",
        "hashtag": "#tarteaucitron",
        "cookieName": "tarteaucitron",
        "orientation": "bottom",
        "showAlertSmall": true,
        "cookieslist": true,
        "adblocker": false,
        "AcceptAllCta" : false,
        "highPrivacy": true,
        "handleBrowserDNTRequest": true,
        "removeCredit": true,
        "moreInfoLink": true
      });

      tarteaucitron.user.analyticsUa = 'UA-44915246-1';
      (tarteaucitron.job = tarteaucitron.job || []).push('analytics');
      $(".menu-lang").click(function(event) {
        $(this).toggleClass("hover");
        event.stopPropagation();
      });
    </script>
