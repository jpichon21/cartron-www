        </span>
        <script>
          $('.header .toggle').on('click', function() { $('.header .menu-top').toggleClass('active'); });
        </script>
        <a class="logo" href="{{ path('homepage') }}">
          <img src="{{ asset('site/images/logo-header.png') }}" alt="Joseph Cartron" />
        </a>
        {% set targetLocale = app.request.locale == 'fr' ? 'en' : 'fr' %}
        <div class="menu-lang" >
          {{ app.request.locale }}
          <a class="choose-lang" href="{{ path('choose_locale', {'locale' : targetLocale}) }}">{{ targetLocale }}</a>
        </div>
        <a class="menu-pro" href="/pro{% if (app.request.locale  == 'en') %}/en{% endif %}">
          {{ 'Espace pro' | trans | raw }}
        </a>
        <div class="menu-top">
          <a class="patrimoine" href="{{ path('static_page',{'page':'Patrimoine'}) }}">{{ "Patrimoine" |trans }}</a>
          <a class="savoir-faire" href="{{ path('static_page',{'page':'Savoir-faire'}) }}">{{ "Savoir-faire"| trans }}</a>
          <a {% if app.request.get('_route') == 'collection' or  app.request.get('_route') == 'collection_produit' %}class="active"{% endif %} href="{{ path('collection') }}">{{ "Collection"| trans }}</a>
          <a {% if app.request.get('_route') == 'degustation' or app.request.get('_route') == 'degustation_produit' %}class="active"{% endif %} href="{{ path('tasting') }}">{{ "Dégustation" | trans }}</a>
          <div class="menu-plus">
            <a href="{{ path('contact') }}">{{ "Contactez-nous" | trans }}</a>
            <a href="#mentions-legales" class="mentions-legales">{{ "Mentions légales" | trans }}</a>
          </div>
        </div>
        <div class="menu-top-right">
          <a href="{{ path('contact') }}" class="contactez-nous">{{ "Contactez-nous" | trans }}</a>
          <a href="/pro{% if(app.request.locale =='en') %}/en{%endif %}" class="espace-pro-header">{{ "Espace" | trans }}<br /><strong>{{ "professionnel" | trans }}</strong></a>
        </div>
      </div>
      {% block body %}{% endblock %}
      {% include 'footer.html.twig'  %} 
